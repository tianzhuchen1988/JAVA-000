<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.sharding.demo.mapper.OrderMapper">

    <select id="findById" resultType="com.example.sharding.demo.entity.Order">
        SELECT *
        FROM t_order where order_id = #{id} and user_id = #{id}
    </select>
</mapper>